{
  "cniVersion": "1.0.0",
  "name": "mynet",
  "plugins": [
        {
            "name": "lo",
            "type": "loopback",
            "sysctl": {
                "net.ipv4.conf.lo.route_localnet": true
            }
        },
        {
            "type": "bridge",
            "bridge": "cni0",
            "ipam": {
                "type": "host-local",
                "subnet": "10.10.0.0/16",
                "gateway": "10.10.0.1",
                "routes": [
                    {"dst": "0.0.0.0/0"}
                ]
            },
            "sysctl": {
                "net.ipv4.conf.cni0.route_localnet": true
            } 
        },
        {
            "type": "portmap",
            "capabilities": {"portMappings": true},
            "sysctl": {
                "net.ipv4.conf.cni0.route_localnet": true
            }
        }
    ]
}
